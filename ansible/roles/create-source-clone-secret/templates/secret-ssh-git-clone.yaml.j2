kind: Secret
apiVersion: v1
metadata:
  name: git-clone-secret
  namespace: {{ build_namespace }}
  labels:
    credential.sync.jenkins.openshift.io: 'true'
  annotations:
    build.openshift.io/source-secret-match-uri-1: '{{ git_url }}'
data:
  ssh-privatekey: >-
    {{ ssh_private_key_value }}
type: kubernetes.io/ssh-auth
